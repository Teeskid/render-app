{
	"database": {
		"rules": "database.json"
	},
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": [
		{
			"source": "cld",
			"codebase": "default",
			"ignore": [
				"venv",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log"
			]
		}
	],
	"hosting": [
		{
			"site": "techify-ng",
			"public": "app/dist",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		}
	],
	"storage": {
		"rules": "storage.json"
	},
	"emulators": {
		"auth": {
			"port": 9099
		},
		"functions": {
			"port": 5001
		},
		"firestore": {
			"port": 8080
		},
		"database": {
			"port": 9000
		},
		"hosting": {
			"port": 5000
		},
		"pubsub": {
			"port": 8085
		},
		"storage": {
			"port": 9199
		},
		"ui": {
			"enabled": true
		},
		"singleProjectMode": true
	},
	"remoteconfig": {
		"template": "config.json"
	},
	"extensions": {}
}